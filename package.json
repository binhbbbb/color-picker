{
  "name": "@fooloomanzoo/color-picker",
  "version": "1.0.0",
  "authors": [
    "Johannes Brautzsch <fooloomanzoo>"
  ],
  "description": "A picker for color, that can use the native input.",
  "homepage": "https://github.com/fooloomanzoo/color-picker/",
  "keywords": [
    "webcomponents",
    "polymer",
    "picker",
    "color",
    "color",
