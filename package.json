    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.2.0",
    "cheerio": "^1.0.0-rc.2",
    "html-minifier": "^3.5.5",
    "polymer-analyzer": "^2.2.2",
    "polymer-bundler": "^3.0.1",
    "uglify-js": "^3.1.1"
  }
